
<div class="members">
<% VotingMemberGoogle.fetchAll.each do |member| %>
	<div class="well">
		<div class="wrapper">
	    	<div class="profile-pic inner">
				<%= image_tag member.picture_url, :class => 'circular'%>
			</div>
			<div class="inner">
				<div class="name-box">
						<%= member.first_name + ' ' + member.last_name%>
				</div>
				<div class="member-description" >
					<%= member.description %>
				</div>
			</div>
		</div>
	</div>
<% end %>
</div>